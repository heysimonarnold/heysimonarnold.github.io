---
import BaseLayout from "./BaseLayout.astro";
import ContentEnd from "./components/ContentEnd.astro";

interface Props {
  title: string;
  type: string;
}

const { title, type } = Astro.props;
---

<BaseLayout title={title} type={type}>
  <div class="text-content-wrapper">
    <slot />
  </div>

  <ContentEnd />
</BaseLayout>
